{
  "name": "iroco-landing",
  "version": "0.1.0",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "build-prod": "vite build --mode production",
    "build-preprod": "vite build --mode preprod",
    "postbuild": "npx svelte-sitemap --domain https://iroco.co",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "test:watch": "vitest",
    "test": "vitest --run",
    "lint": "prettier --plugin-search-dir . --check . && eslint .",
    "format": "prettier --plugin-search-dir . --write .",
    "clean": "rm -rf .svelte-kit build && npm cache clean --force",
    "prepare": "husky"
  },
  "devDependencies": {
    "@sveltejs/adapter-auto": "^4.0.0",
    "@sveltejs/adapter-static": "^3.0.8",
    "@sveltejs/kit": "^2.16.0",
    "@sveltejs/vite-plugin-svelte": "^5.0.3",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/svelte": "^5.2.6",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.28.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-svelte": "^2.45.1",
    "eslint-plugin-vitest-globals": "^1.5.0",
    "husky": "^9.0.11",
    "happy-dom": "^16.6.0",
    "lodash-es": "^4.17.21",
    "msw": "^1.3.2",
    "prettier": "^3.4.2",
    "prettier-plugin-svelte": "^3.3.3",
    "sass": "^1.83.4",
    "svelte": "^5.18.0",
    "svelte-check": "^4.1.4",
    "svelte-preprocess": "^6.0.3",
    "svelte-sitemap": "^2.7.0",
    "tslib": "^2.8.1",
    "typescript": "^5.7.3",
    "vite": "^6.0.7",
    "vitest": "^3.0.2"
  },
  "dependencies": {
    "@iroco/ui": "1.1.1",
    "svelte-awesome": "^3.3.5",
    "svelte-i18n": "^4.0.1"
  },
  "lint-staged": {
    "*.{js,ts,svelte}": [
      "eslint --fix",
      "prettier --write -u"
    ],
    "!**/*.{js,ts,jsx,tsx}": "prettier --write -u"
  },
  "type": "module"
}
